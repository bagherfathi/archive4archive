<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.sm.entity" >
	<class
		name="Operator"
		table="SM_OPERATOR"
		schema="RFMS"
		lazy="false"
	>
		<id
			name="operatorId"
			type="long"
			column="OPERATOR_ID"
		>
			<generator class="increment"/>
		</id>

		<property
			name="orgId"
			column="ORG_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="opName"
			column="OP_NAME"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>
		<property
			name="loginName"
			column="LOGIN_NAME"
			type="java.lang.String"
			not-null="false"
			length="32"
		/>
		<property
			name="type"
			column="TYPE"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="lockStatus"
			column="LOCK_STATUS"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="memo"
			column="MEMO"
			type="java.lang.String"
			not-null="false"
			length="255"
		/>
		<property
			name="regionId"
			column="REGION_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="email"
			column="EMAIL"
			type="java.lang.String"
			not-null="false"
			length="255"
		/>
		<property
			name="msn"
			column="MSN"
			type="java.lang.String"
			not-null="false"
			length="255"
		/>
		<property
			name="password"
			column="PASSWORD"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>
		<property
			name="status"
			column="STATUS"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="address"
			column="ADDRESS"
			type="java.lang.String"
			not-null="false"
			length="255"
		/>
		<property
			name="mobilePhone"
			column="MOBILE_PHONE"
			type="java.lang.String"
			not-null="false"
			length="32"
		/>
		<property
			name="telePhone"
			column="TELE_PHONE"
			type="java.lang.String"
			not-null="false"
			length="32"
		/>
		<property
			name="postcode"
			column="POSTCODE"
			type="java.lang.String"
			not-null="false"
			length="12"
		/>
		<property
			name="regType"
			column="REG_TYPE"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="regNumber"
			column="REG_NUMBER"
			type="java.lang.String"
			not-null="false"
			length="32"
		/>
		<property
			name="createTime"
			column="CREATE_TIME"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="modifyTime"
			column="MODIFY_TIME"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="validTime"
			column="VALID_TIME"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="expireTime"
			column="EXPIRE_TIME"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="ssoCode"
			column="SSO_CODE"
			type="java.lang.String"
			not-null="false"
			length="32"
		/>
		<property
			name="jobNumber"
			column="JOB_NUMBER"
			type="java.lang.String"
			not-null="false"
			length="16"
		/>
		<property
			name="orgSsoCode"
			column="ORG_SSO_CODE"
			type="java.lang.String"
			not-null="false"
			length="32"
		/>
		<property
			name="ssoStatus"
			column="SSO_STATUS"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="ssoAccessed"
			column="SSO_ACCESSED"
			type="long"
			not-null="false"
			length="2"
		/>
         <property
			name="merchantCode"
			column="MERCHANT_CODE"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>

	</class>	
</hibernate-mapping>
