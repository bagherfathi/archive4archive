<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.rfms.entity">
	<class
		name="RfcsPosSignin"
		table="RFCS_POS_SIGNIN"  lazy="false"
	>
		<meta attribute="sync-DAO">true</meta>

		<id
			name="signinId"
			type="java.lang.Long"
			column="SIGNIN_ID"
		>
			<generator class="sequence">
			    <param name="sequence">RFMS.RFCS_POS_SIGNIN_SEQ</param>
			</generator>
		</id>
		<property
			name="operatorId"
			column="OPERATOR_ID"
			type="java.lang.Long"
			not-null="false"
			length="14"
		/>
		<property
			name="posId"
			column="POS_ID"
			type="java.lang.Long"
			not-null="false"
			length="14"
		/>
		<property
			name="pinKey"
			column="PIN_KEY"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="macKey"
			column="MAC_KEY"
			type="string"
			not-null="false"
			length="256"
		/>
		<property
			name="signinTime"
			column="SIGNIN_TIME"
			type="date"
			not-null="false"
			length="7"
		/>


	</class>	
</hibernate-mapping>