							<tr> 
								<td bgcolor="#eeeee5" width="20%">公交站点名称:
								</td>
								<td>
									#showFormText("name" "$!bizObj.name" 40)#showMustFill()
								</td>
							</tr>
							<tr> 
								<td bgcolor="#eeeee5" width="20%">描述:</td>
								<td>
									#showFormTextarea("content" "$!bizObj.content" 5)#showMustFill()
