							</tr>
							<tr> 
								<td bgcolor="#eeeee5" width="20%">所属公交线路id:
								</td>
								<td>
									#showFormText("busId" "$!bizObj.busId" 40)#showMustFill()
								</td>
							</tr>
							<tr> 
								<td bgcolor="#eeeee5" width="20%">坐标X:
								</td>
								<td>
									#showFormText("placeX" "$!bizObj.placeX" 40)#showMustFill()
