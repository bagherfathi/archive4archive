<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.rfms.entity">
	<class
		name="RfmsDictSettleType"
		table="RFMS_DICT_SETTLE_TYPE" schema="RFMS" lazy="false"
	>
		<meta attribute="sync-DAO">true</meta>
		<id
			name="id"
			type="java.lang.Long"
			column="ID"
		>
			<generator class="sequence">
			    <param name="sequence">RFMS.RFMS_DICT_SETTLE_TYPE_SEQ</param>
			</generator>
		</id>

		<property
			name="text"
			column="TEXT"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="english"
			column="ENGLISH"
			type="string"
			not-null="false"
			length="200"
		/>
		<property
			name="iconurl"
			column="ICONURL"
			type="string"
			not-null="false"
			length="200"
		/>
 


	</class>	
</hibernate-mapping>