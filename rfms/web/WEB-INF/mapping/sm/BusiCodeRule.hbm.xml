			type="java.lang.String"
			not-null="false"
			length="63"
		/>
		<property
			name="maxValue"
			column="MAX_VALUE"
			type="java.lang.String"
			not-null="false"
			length="63"
		/>
		<property
			name="needWad"
			column="NEED_WAD"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="wadChar"
			column="WAD_CHAR"
			type="java.lang.String"
			not-null="false"
			length="1"
