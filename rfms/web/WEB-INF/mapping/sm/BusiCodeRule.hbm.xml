		<property
			name="updateDay"
			column="UPDATE_DAY"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="updateOp"
			column="UPDATE_OP"
			type="java.lang.String"
			not-null="false"
			length="63"
		/>
		<property
			name="createDay"
			column="CREATE_DAY"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="createOp"
			column="CREATE_OP"
			type="java.lang.String"
			not-null="false"
			length="63"
		/>


	</class>	
</hibernate-mapping>
