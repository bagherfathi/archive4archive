<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.sm.entity" >
	<class
		name="LockObj"
		table="SM_LOCK_OBJ"
		schema="RFMS"
		lazy="false"
	>
		<id
			name="lockObjId"
			type="long"
			column="LOCK_OBJ_ID"
		>
			<generator class="increment"/>
		</id>

		<property
			name="lockObjName"
			column="LOCK_OBJ_NAME"
			type="java.lang.String"
			not-null="true"
			length="128"
		/>
		<property
			name="lockObjCode"
			column="LOCK_OBJ_CODE"
			type="java.lang.String"
			not-null="true"
			length="63"
		/>
		<property
			name="updateDate"
			column="UPDATE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>


	</class>	
</hibernate-mapping>
