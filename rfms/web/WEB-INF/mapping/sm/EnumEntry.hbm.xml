<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.sm.entity" >
	<class
		name="EnumEntry"
		table="SM_ENUM_ENTRY"
		schema="RFMS"
		lazy="false"
	>
		<id
			name="enumEntryId"
			type="long"
			column="ENUM_ENTRY_ID"
		>
			<generator class="increment"/>
		</id>

		<property
			name="enumId"
			column="ENUM_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="enumEntryLabel"
			column="ENUM_ENTRY_LABEL"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>
		<property
			name="enumEntryValue"
			column="ENUM_ENTRY_VALUE"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>
		<property
			name="enumEntryStatus"
			column="ENUM_ENTRY_STATUS"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="entryLinkValue"
			column="ENTRY_LINK_VALUE"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>
		<property
			name="appId"
			column="APP_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="validDate"
			column="VALID_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="expireDate"
			column="EXPIRE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="updateDate"
			column="UPDATE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="operatorId"
			column="OPERATOR_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="orgId"
			column="ORG_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="loginOrgId"
			column="LOGIN_ORG_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="entryOrder"
			column="ENTRY_ORDER"
			type="long"
			not-null="false"
			length="4"
		/>


	</class>	
</hibernate-mapping>
