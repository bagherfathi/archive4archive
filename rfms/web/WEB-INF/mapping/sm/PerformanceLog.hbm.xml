<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.sm.entity" >
	<class
		name="PerformanceLog"
		table="SM_PERFORMANCE_LOG"
		schema="RFMS"
		lazy="false"
	>
		<id
			name="performanceLogId"
			type="long"
			column="PERFORMANCE_LOG_ID"
		>
			<generator class="increment"/>
		</id>

		<property
			name="logId"
			column="LOG_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="startTime"
			column="START_TIME"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="endTime"
			column="END_TIME"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>


	</class>	
</hibernate-mapping>
