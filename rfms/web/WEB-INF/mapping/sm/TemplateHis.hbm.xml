<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.sm.entity" >
	<class
		name="TemplateHis"
		table="SM_TEMPLATE_HIS"
		schema="RFMS"
		lazy="false"
	>
		<id
			name="historyId"
			type="long"
			column="HISTORY_ID"
		>
			<generator class="increment"/>
		</id>

		<property
			name="templateId"
			column="TEMPLATE_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="templateName"
			column="TEMPLATE_NAME"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>
		<property
			name="templateCode"
			column="TEMPLATE_CODE"
			type="java.lang.String"
			not-null="false"
			length="32"
		/>
		<property
			name="categoryCode"
			column="CATEGORY_CODE"
			type="java.lang.String"
			not-null="false"
			length="64"
		/>
		<property
			name="templateDesc"
			column="TEMPLATE_DESC"
			type="java.lang.String"
			not-null="false"
			length="255"
		/>
		<property
			name="appId"
			column="APP_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="validDate"
			column="VALID_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="expireDate"
			column="EXPIRE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="updateDate"
			column="UPDATE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="operatorId"
			column="OPERATOR_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="orgId"
			column="ORG_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="loginOrgId"
			column="LOGIN_ORG_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="newAppId"
			column="NEW_APP_ID"
			type="long"
			not-null="false"
			length="14"
		/>


	</class>	
</hibernate-mapping>
