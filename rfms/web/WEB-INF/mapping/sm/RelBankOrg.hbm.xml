<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.sm.entity" >
	<class
		name="RelBankOrg"
		table="SM_REL_BANK_ORG"
		
		lazy="false"
	>
		<id
			name="relId"
			type="long"
			column="rel_id"
		>
			<generator class="increment"/>
		</id>

		<property
			name="bankId"
			column="bank_id"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="bankOrgId"
			column="bank_org_id"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="onlineStatus"
			column="online_status"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="onlineType"
			column="online_type"
			type="long"
			not-null="false"
			length="2"
		/>
		<property
			name="appId"
			column="app_id"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="createDate"
			column="create_date"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="validDate"
			column="valid_date"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="expireDate"
			column="expire_date"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="updateDate"
			column="update_date"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="operatorId"
			column="operator_id"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="orgId"
			column="org_id"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="loginOrgId"
			column="login_org_id"
			type="long"
			not-null="false"
			length="14"
		/>


	</class>	
</hibernate-mapping>
