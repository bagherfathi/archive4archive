<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
	"-//Hibernate/Hibernate Mapping DTD//EN"
	"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >

<hibernate-mapping package="com.ft.sm.entity" >
	<class
		name="TemplateFile"
		table="SM_TEMPLATE_FILE"
		schema="RFMS"
		lazy="false"
	>
		<id
			name="fileId"
			type="long"
			column="FILE_ID"
		>
			<generator class="increment"/>
		</id>

		<property
			name="templateId"
			column="TEMPLATE_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="fileVersion"
			column="FILE_VERSION"
			type="long"
			not-null="false"
			length="4"
		/>
		<property
			name="fileContent"
			column="FILE_CONTENT"
			type="org.springframework.orm.hibernate3.support.BlobByteArrayType"
			not-null="false"
		/>
		<property
			name="createDate"
			column="CREATE_DATE"
			type="java.util.Date"
			not-null="false"
			length="7"
		/>
		<property
			name="operatorId"
			column="OPERATOR_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="orgId"
			column="ORG_ID"
			type="long"
			not-null="false"
			length="14"
		/>
		<property
			name="loginOrgId"
			column="LOGIN_ORG_ID"
			type="long"
			not-null="false"
			length="14"
		/>


	</class>	
</hibernate-mapping>
